{
  "name": "ide-gladiator-conf",
  "main": "./build/main.js",
  "version": "0.0.0",
  "description": "Extension providing support for wirting Gladiator configuration files.",
  "keywords": [
    "yaml",
    "ide-yaml",
    "schema"
  ],
  "repository": "https://git.kpi.fei.tuke.sk/rk740eq/ide-gladiator-conf",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0",
    "node": "*"
  },
  "scripts": {
    "build": "tsc",
    "clean": "del-cli ./lib/*.js"
  },
  "dependencies": {
    "@types/is-glob": "^4.0.1",
    "atom-languageclient": "^0.9.9",
    "etch": "^0.14.0",
    "glob": "^7.1.3",
    "is-glob": "^4.0.1",
    "vscode-jsonrpc": "^4.0.0",
    "vscode-languageserver-protocol": "^3.15.0-next.1",
    "vscode-languageserver-types": "^3.14.0",
    "yaml-ast-parser": "0.0.43",
    "yaml-language-server": "^0.3.2"
  },
  "devDependencies": {
    "@types/atom": "^1.31.1",
    "@types/glob": "^7.1.1",
    "@types/node": "^11.11.5",
    "del-cli": "^1.1.0",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  },
  "enhancedScopes": [
    "source.yaml",
    "source.yml"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "atomRequirements": [
    "atom-ide-ui"
  ]
}
