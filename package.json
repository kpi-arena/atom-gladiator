{
  "name": "ide-gladiator-conf",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Atom IDE implementation of gladiator-conf-language-server ",
  "keywords": [
    "yaml",
    "ide-yaml",
    "schema"
  ],
  "repository": "https://git.kpi.fei.tuke.sk/rk740eq/ide-gladiator-conf",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0",
    "node": "*"
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "atom-languageclient": "^0.9.9",
    "yaml-language-server": "^0.3.2"
  },
  "devDependencies": {
    "@types/node": "^11.11.5",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  },
  "enhancedScopes": [
    "source.yaml",
    "source.yml"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "atomRequirements": [
    "atom-ide-ui"
  ]
}
